    <main>
        <div class="dashboard-header" id="dashboard-header">
            <h1>Dashboard</h1>
            {{#if scores.length}}
              <section>
                <h2>Your Posts</h2>
                <ol>
                  {{#each scores as |score|}}
                    {{>score-post score}}
                  {{/each}}
                </ol>
              </section>
            {{/if}}
        </div>
    </main>
    <footer class="page-footer main-background">
        <ul class="nav nav-pills nav-fill">
