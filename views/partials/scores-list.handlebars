<div class="card highscore-card container-fluid" id="highscore-card" >
    <div class="card-body">
        <div class="quiz-name" id="quiz-name">
            <p>{{user.username}}</p>
        </div>
        <div class="quiz-date" id="quiz-date">
            <p>{{format_date created_at}}</p>
        </div>
        <div class="score-display" id="score-display">
            <p>{{score_amount}} </p>
        </div>
    </div>
</div>